<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="400.0" maxWidth="600.0" minHeight="400.0" minWidth="600.0" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mcaccountchecker.controllers.MainController">
   <children>
      <TextArea fx:id="accountList" layoutX="14.0" layoutY="14.0" prefHeight="347.0" prefWidth="248.0" promptText="username:password" />
      <TableView fx:id="validAccountTable" layoutX="277.0" layoutY="34.0" prefHeight="326.0" prefWidth="311.0">
        <columns>
          <TableColumn fx:id="usernameColumn" editable="false" prefWidth="159.0" text="Username" />
          <TableColumn fx:id="passwordColumn" prefWidth="151.0" text="Password" />
        </columns>
      </TableView>
      <Button fx:id="startButton" layoutX="14.0" layoutY="367.0" mnemonicParsing="false" onAction="#startButtonClick" prefHeight="25.0" prefWidth="122.0" text="Start" />
      <Button fx:id="resetButton" layoutX="139.0" layoutY="367.0" mnemonicParsing="false" onAction="#resetButtonClick" prefHeight="25.0" prefWidth="122.0" text="Reset" />
      <Label layoutX="384.0" layoutY="8.0" text="Valid Accounts">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <ProgressBar fx:id="progressBar" layoutX="277.0" layoutY="371.0" prefHeight="18.0" prefWidth="311.0" progress="0.0" />
   </children>
</AnchorPane>
